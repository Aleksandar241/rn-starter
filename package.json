  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "detectCircularDependencies": "dpdm --no-warning --no-tree ./src/*",
    "icon:gen": "node scripts/icon-gen.js",
    "comp:gen": "node scripts/component-gen.js $1",
    "lint": "expo lint",
    "test": "jest --watchAll --coverage",
